{
    "name": "plenny/visitor",
    "description": "Bridge package for Vue apps with Laravel backend.",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Rafa≈Ç Krawiec",
            "email": "rafal.krawiec16@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-pcntl": "*",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Plenny\\Visitor\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Plenny\\Visitor\\VisitorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
